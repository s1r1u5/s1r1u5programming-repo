#include <stdio.h>

float myFunction(float x);

int main()
{
	float inputValue, outputValue;

	inputValue = 42.0;

	outputValue = myFunction(inputValue);

	printf("Results are: %f", outputValue);

	return 0;
}

myFunction(float x)
{
	return x * 42.0;
}
