#include <stdio.h>

int main()
{
	float lifeTimeMiles = 0;															/* Defining Variables */
	float lifeTimeGallons = 0;
	float lastTankMiles = 0;
	float lastTankGallons = 0;
	int lifeTimeFillups = 0;
	float lifeTimeMPG = lifeTimeMiles / lifeTimeGallons;
	float lastTankMPG = lastTankMiles / lastTankGallons;

	printf("Type the number of miles driver on this tank gas: ");						/* Setting lastTankMiles */
	fflush(stdout);
	scanf("%f", lastTankMiles);

	printf("Type the number of gallons in this tank: ");
	fflush(stdout);
	scanf("%f", lastTankGallons);														/* Setting lastTankGallons */

	while (1 == 1)																		/* Starting Repeating Loop */
	{	printf("RUNNING WHILE LOOP");
		fflush(stdout);	}
	}

return 0;
}
